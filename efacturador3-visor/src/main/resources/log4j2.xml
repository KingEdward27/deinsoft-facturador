<Configuration name="ConfigTest" status="ERROR" monitorInterval="5">
    <Appenders>
 
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout
                pattern="%d{yyyy-MMM-dd HH:mm:ss a} [%t] %-5level %logger{36} - %msg%n" />
        </Console>
 
    </Appenders>
    <Loggers>
        <Logger name="com.deinsoft.efacturador3commons" level="trace" additivity="false">
            <AppenderRef ref="Out"/>
        </Logger>
        <Root level="error">
            <AppenderRef ref="Out"/>
        </Root>
    </Loggers>
</Configuration>